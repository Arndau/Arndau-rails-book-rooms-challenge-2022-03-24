<div class="cardscontainer">
  <% @rooms.each do |room| %>
  <div class="card-product">
  <img src="https://www.nycinsiderguide.com/wp-content/uploads/2018/05/pod-39-hotel-nyc-bunk-beds.jpg" />
  <div class="card-product-infos">
          <%= room.hotel.address%>
          <%= room.price_per_night %>
        </div>
          <%= link_to "Voir le chambre",room_path(room) %>
      </div>
  <% end %>
</div>
